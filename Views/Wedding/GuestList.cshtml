@{
    ViewData["Title"] = "Guest List";
} 
@model WeddingPlanner.Models.Wedding
@{
<div class="row guestlist">
    <div class="col-md-6">
        <h1>@Model.Wedder1 and @Model.Wedder2's Wedding</h1>
        <h2>@Model.WeddingDate.ToString("MMM dd, yyyy hh:mm tt")</h2>
        <h2>Guests: </h2>
        @foreach(var item in @Model.GuestLists){
            <p class="names">@item.WeddingAttendee.FirstName @item.WeddingAttendee.LastName </p>
        }
    </div>
    <div class="col-md-6">
        <iframe
            width="600"
            height="450"
            frameborder="0" style="border:0"
            src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBFfAg3BFYAgtC-5fXvO78uxOUlxcd6Q1k&q=@Model.Address" allowfullscreen>
        </iframe>
    </div>
</div>
}